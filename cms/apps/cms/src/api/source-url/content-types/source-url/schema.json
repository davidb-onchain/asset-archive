{
  "collectionName": "source_urls",
  "info": {
    "singularName": "source-url",
    "pluralName": "source-urls",
    "displayName": "SourceUrl"
  },
  "options": { "draftAndPublish": true },
  "attributes": {
    "url": { "type": "string", "required": true, "unique": true },
    "type": { "type": "enumeration", "enum": ["assetstore", "publisher", "docs", "video", "community", "other"] },
    "discoveredBy": { "type": "enumeration", "enum": ["cache", "embedded", "name-heuristics", "guid-fingerprint", "online-search"] },
    "confidence": { "type": "decimal" },
    "firstSeenAt": { "type": "datetime" },
    "lastSeenAt": { "type": "datetime" },
    "notes": { "type": "text" },
    "products": { "type": "relation", "relation": "manyToMany", "target": "api::product.product", "inversedBy": "sourceUrls" },
    "archiveSnapshots": { "type": "relation", "relation": "oneToMany", "target": "api::archive-snapshot.archive-snapshot", "mappedBy": "sourceUrl" }
  }
}
