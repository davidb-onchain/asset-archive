    "sizeBytes": { "type": "biginteger" },
    "mimeType": { "type": "string" },
    "checksum": { "type": "string" },
    "quickKey": { "type": "string" },
    "pageUrl": { "type": "string" },
    "directUrl": { "type": "string" },
    "folderKey": { "type": "string" },
    "status": { "type": "enumeration", "enum": ["uploaded", "failed"] },
    "createdAtRemote": { "type": "datetime" },
    "products": { "type": "relation", "relation": "manyToMany", "target": "api::product.product" }
  }
}
